{
  "manifest_version": 2,
  "name": "Home Art",
  "version": "0.1",

  "background": {
    "scripts": ["content.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["chrome://newtab"],
      "css": ["style.css"],
      "js": ["content.js"]
    }
  ],

  "chrome_url_overrides": {
    "newtab": "new_home_.html"
  }
}
